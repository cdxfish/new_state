<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="add_major_type">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object" name="professional_edit" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="management_delete" confirm="您确定要删除吗？" >删除</button>
        </div>
    </t>

    <t t-name="add_class_type">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object" name="class_edit" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="class_delete" confirm="您确定要删除吗？" >删除</button>
        </div>
    </t>

    <t t-name="regulations_management_search_button">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">线路:</span>
                    <span for="line_id" class="col-xs-8"></span>
                </div>
                <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">站点:</span>
                    <span for="site_id" class="col-xs-8"></span>
                </div>
                <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">专业分类:</span>
                    <span for="profession_kind" class="col-xs-8"></span>
                </div>
                <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">级别分类:</span>
                    <span for="rank_kind" class="col-xs-8"></span>
                </div>
                <div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px;height:40px; line-height:40px;">
                    <button type="button" class="btn btn-default search_extend_apply">
                        搜索
                    </button>
                </div>
            </div>
        </div>
    </t>
</templates>
