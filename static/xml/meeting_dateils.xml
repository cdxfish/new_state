<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
	<!--修改与删除按钮和删除-->
    <t t-name="meet_details_button">
        <div class="layui-btn-group">
            <button type="object" hidden="hidden" name="meet_dateils_button"  class="layui-btn layui-btn-sm"
                    group="funenc_xa_station.meet_record_details_button">详情</button>
            <button class="layui-btn layui-btn-sm" type="object" hidden="hidden" name="meet_change_button"
                    group="funenc_xa_station.meet_record_Modify_button">修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object" group="funenc_xa_station.meet_record_delete_button"
                      name="meet_delete_button" confirm="您确定要删除该条记录吗？" >删除</button>
        </div>
    </t>

    <t t-name="meet_details_search_button">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">会议时间:</label>
                    <div for="meeting_time" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">站点:</label>
                    <div for="line_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">归属站点:</label>
                    <div for="site_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:200px;">
                    <button type="button" class="btn btn-default search_extend_apply">
                        搜索
                    </button>
                    <button type="button" class="btn btn-default search_extend_reset">
                        重置
                    </button>
                </div>
            </div>
        </div>
    </t>
</templates>
