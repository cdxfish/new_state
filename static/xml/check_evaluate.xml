<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="check_evaluate_button">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object"  name="check_evalulate_edit" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="check_evaluate_delete" confirm="您确定要删除吗？" >删除</button>
        </div>
    </t>

    <t t-name="award_standard_button">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object"  name="award_edit" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="award_delete" confirm="您确定要删除吗？" >删除</button>
        </div>
    </t>
    <t t-name="check_record_add">
        <div class="layui-btn-group">
                <button class="layui-btn layui-btn-sm" type="object"  name="award_delete">新增责任人</button>
        </div>
    </t>

    <t t-name="check_record_edit">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object" name="check_record_change"
            group='funenc_xa_station.check_management_check_record_modify'>修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="check_record_delete"
                    confirm="您确定要删除吗？"  group='funenc_xa_station.check_management_check_record_delete'>删除</button>
        </div>
    </t>

    <t t-name="award_record_edit">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object" name="check_record_change"
            group='funenc_xa_station.check_management_award_record_modify'>修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="check_record_delete"
                    confirm="您确定要删除吗？" group='funenc_xa_station.check_management_award_record_delete'>删除</button>
        </div>
    </t>

    <t t-name="check_record_search_button">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">日期筛选:</label>
                    <div for="check_time" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">线路:</label>
                    <div for="line_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">站点:</label>
                    <div for="site_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">人员搜索:</label>
                    <div for="staff" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:200px;">
                    <button type="button" class="btn btn-default search_extend_apply">
                        搜索
                    </button>
                </div>
            </div>
        </div>
    </t>
    <t t-name="award_record_search_button">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">日期筛选:</label>
                    <div for="check_time" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">线路:</label>
                    <div for="line_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">站点:</label>
                    <div for="site_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
                    <label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">人员搜索:</label>
                    <div for="staff" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
                </div>
                <div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:200px;">
                    <button type="button" class="btn btn-default search_extend_apply">
                        搜索
                    </button>
                </div>
            </div>
        </div>
    </t>
</templates>
