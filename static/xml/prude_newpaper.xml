<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
	<!--修改与删除按钮和删除-->
    <t t-name="prude_newpaper_action">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object" group="funenc_xa_station.new_production_newpaper_Modify_button"
                    hidden="hidden"
                    name="change_change_act" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger"   group='funenc_xa_station.new_production_newpaper_delete_button'
                    type="object" name="delete_button_action" hidden="hidden"
                    confirm="您确定要删除该条记录吗？">删除</button>
        </div>
    </t>
    <t t-name="prude_newpaper_type_action">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object"  name="prude_newpaper_type_onchange" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="prude_newpaper_type_selete"
                    confirm="您确定要删除该条记录吗？">删除</button>
        </div>
    </t>

    <t t-name="prude_newpaper_write_action">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object"  name="prude_newpaper_type_onchange" >修改</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" type="object"  name="prude_newpaper_type_selete"
                    confirm="您确定要删除该条记录吗？">删除</button>
        </div>
    </t>

    <t t-name="product_search_button">
		<div class="form-inline custom_search_box">
			<div class="container-fluid">
				<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
					<label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">发生时间:</label>
					<div for="open_time" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
				</div>
				<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
					<label class="search_label col-lg-3 col-md-4 col-sm-4 col-xs-4">站点:</label>
					<div for="line_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
				</div>
				<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:300px;">
					<label class="search_label col-lg-6 col-md-6 col-sm-6 col-xs-6">归属站点:</label>
					<div for="site_id" class="form-widget-item col-md-5 col-sm-5 col-xs-8"/>
				</div>
				<div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; width:200px;">
					<button type="button" class="btn btn-default search_extend_apply">
						搜索
					</button>
					<button type="button" class="btn btn-default search_extend_reset">
						重置
					</button>
				</div>
			</div>
		</div>
	</t>
</templates>
