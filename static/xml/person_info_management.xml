<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <!--人员信心详情显示按钮-->
    <t t-name="person_info_management">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-radius layui-btn-primary" type="object"  name="person_info" >查看详情</button>
        </div>
    </t>

        <!--人员信心详情显示按钮-->
    <t t-name="person_certificate_management">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-radius layui-btn-primary" type="object"  name="cer" >查看详情</button>
        </div>
    </t>
    <t t-name="person_certificate_operation">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-radius layui-btn-primary" type="object"  name="person_info" >查看详情</button>
        </div>
    </t>
    <t t-name="create_cerficate_button">
        <div class="layui-btn-group">
            <button class="layui-btn" type="object"  name="person_cer_edit" >新建证件</button>
        </div>
    </t>

    <t t-name="new_increase_cerfiticase_information">
        <div class="layui-btn-group">
        <button class="layui-btn layui-btn-sm" type="object"  name="new_increase_cerfiticase_infor" >新建证件</button>
        </div>
    </t>

    <t t-name="new_increase_jobt_ranfer_information">
        <div class="layui-btn-group">
        <button class="layui-btn layui-btn-sm" type="object"  name="new_increase_jobt_ranfer_infor" >新建转岗</button>
        </div>
    </t>

    <t t-name="second_details">
        <div class="layui-btn-group">
            <button class="layui-btn" type="object" hidden="hidden" name="second_modification"
            group='funenc_xa_station.person_secondment_new_modify'>修改</button>
            <button class="layui-btn" type="object" hidden="hidden" name="second_delect" confirm="您确定要删除该条记录吗？"
            group='funenc_xa_station.person_secondment_delete'>删除</button>
            <button class="layui-btn" type="object" hidden="hidden" name="person_details"
            group='funenc_xa_station.person_secondment_persom_details'>人员详情</button>
        </div>
    </t>

    <t t-name="per_management_summary_search_button">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-3 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">线路:</span>
                    <span for="line_id" class="col-xs-8"></span>
                </div>
                <div class="col-lg-3 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">站点:</span>
                    <span for="department_name" class="col-xs-8"></span>
                </div>
                <div class="col-lg-3 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">人员搜索:</span>
                    <span for="name" class="col-xs-8"></span>
                </div>
                <div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <button type="button" class="btn btn-default search_extend_apply">
                        搜索
                    </button>
                </div>
            </div>
        </div>
    </t>
</templates>