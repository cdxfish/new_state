<odoo>
	<data>
		<record model="ir.ui.view" id="fuenc_station_key_manage_list">
			<field name="name">fuenc_station_key_manage_list</field>
			<field name="model">fuenc.station.key.manage</field>
			<field name="arch" type="xml">
				<tree limit="15">
					<field name="name" string="名称"/>
				</tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="fuenc_station_key_manage_form">
			<field name="name">fuenc_station_key_manage_form</field>
			<field name="model">fuenc.station.key.manage</field>
			<field name="arch" type="xml">
				<form>
					<div class="container-fluid">
						<group>
							<field name="line_id" string="选择线路"/>
							<field name="ascription_site_id" string="归属站点"/>
							<field name="key_type_id" string="钥匙类型"/>
							<field name="key_no" string="钥匙编号" placeholder="可批量,编号之间隔行"/>
							<field name="name" string="名称"/>
							<field name="key_position" string="对应位置"/>
						</group>
					</div>
				</form>
			</field>
		</record>
	
	</data>
</odoo>