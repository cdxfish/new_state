<odoo>
	<data>
		<record model="ir.ui.view" id="fuenc_station_key_manage_list">
			<field name="name">fuenc_station_key_manage_list</field>
			<field name="model">fuenc.station.key.manage</field>
			<field name="arch" type="xml">
				<tree limit="15">
					<field name="id" string="序号"/>
					<field name="line_id"/>
					<field name="ascription_site_id"/>
					<field name="key_type_id"/>
					<field name="master_number"/>
					<field name="copy_number"/>
					<field name="key_total"/>
					<field name="borrow_number"/>
					<field name="destroy_number"/>
				</tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="fuenc_station_key_manage_form">
			<field name="name">fuenc_station_key_manage_form</field>
			<field name="model">fuenc.station.key.manage</field>
			<field name="arch" type="xml">
				<form>
					<div class="container-fluid">
						<group>
							<field name="line_id"/>
							<field name="ascription_site_id"/>
							<field name="key_type_id"/>
							<field name="master_number"/>
							<field name="copy_number"/>
							<field name="key_total"/>
							<field name="borrow_number"/>
							<field name="destroy_number"/>
						</group>
					</div>
				</form>
			</field>
		</record>
	
	</data>
</odoo>