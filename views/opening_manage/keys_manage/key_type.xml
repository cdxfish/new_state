<odoo>
	<data>
		<record model="ir.ui.view" id="fuenc_station_key_type_list">
			<field name="name">fuenc_station_key_type_list</field>
			<field name="model">fuenc.station.key.type</field>
			<field name="arch" type="xml">
				<tree limit="15">
					<field name="name" string="钥匙类型"/>
					<field name="remarks" string="备注"/>
					<!--<field name="operation" string="操作" >-->
					<!--</field>-->
					 <button name="operation" string="修改"/>
					<button name="delete" string="删除"/>

				</tree>
			</field>
		</record>
		
		<record model="ir.ui.view" id="fuenc_station_key_type_form">
			<field name="name">fuenc_station_key_type_form</field>
			<field name="model">fuenc.station.key.type</field>
			<field name="arch" type="xml">
				<form>
					<div class="container-fluid">
						<group>
							<field name="name" string="钥匙类型"/>
							<field name="remarks" string="备注"/>
						</group>
					</div>
				</form>
			</field>
		</record>
	
	</data>
</odoo>