<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="import_file" model="ir.ui.view">
            <field name="name">import_file</field>
            <field name="model">import.management</field>
            <field name="arch" type="xml">
                <tree limit="15">
                    <field name="xls_file"></field>
                </tree>
            </field>
        </record>

        <record id="import_file_form" model="ir.ui.view">
            <field name="name">import_file</field>
            <field name="model">import.management</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="xls_file"></field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="import_file_act_manage" model="ir.actions.act_window">
            <field name="name">import_file_act</field>
            <field name="res_model">import.management</field>
            <field name="view_mode">tree,kanban,form</field>
        </record>

        <!--<menuitem name="import_file_act"  action="import_file_act_manage" id="imporsfsafsafdfs"> </menuitem>-->

    </data>
</odoo>